---
layout: layout.njk
pagination:
  data: collections
  size: 1
  alias: tag
permalink: "tags/{{ tag | slug }}/"
---

<!-- need to edit layout when the njk is done.
 may need to make a separate njk for tag page template-->

<h1>#{{ tag }}</h1>

<ul>
{% for post in collections[tag] %}
  <li>
    <a href="{{ post.url }}">{{ post.data.title }}</a>
  </li>
{% endfor %}
</ul>
